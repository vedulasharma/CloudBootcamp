<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="info">
  <Appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout>
        <Pattern>%d{HH:mm:ss.SSS} ReceivingDispatcher [%c] %-5level - %msg%n</Pattern>
        <Replace regex="\r*\n(?=.*\r*\n)" replacement="; "/>
      </PatternLayout>
    </Console>
  </Appenders>
  <Loggers>
    <Root level="trace">
      <AppenderRef ref="Console" />
    </Root>
  </Loggers>
</Configuration>